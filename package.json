{
  "name": "privaterooms",
  "version": "2.0.0",
  "description": "Private rooms script for discord bot",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "start": "nodemon --config nodemon.json --ext ts --ignore 'src/config/*.json' -r tsconfig-paths/register ./main.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "bundle": "esbuild src/index.ts --bundle --platform=node --target=node16.7 --outfile=bundle/out.js --external:discord.js --external:mongoose",
    "debug": "node --inspect -r ts-node/register -r tsconfig-paths/register ./main.ts"
  },
  "author": "zvykach",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^16.11.0",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.3.10",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zvykach/PrivateRooms.git"
  },
  "bugs": {
    "url": "https://github.com/zvykach/PrivateRooms/issues"
  },
  "homepage": "https://github.com/zvykach/PrivateRooms#readme",
  "dependencies": {
    "discord.js": "^13.3.1",
    "esbuild": "^0.14.0",
    "moment": "^2.29.1",
    "mongoose": "^6.0.13",
    "nodemon": "^2.0.15"
  }
}
